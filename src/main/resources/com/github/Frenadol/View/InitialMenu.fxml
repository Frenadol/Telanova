<VBox alignment="CENTER" prefHeight="301.0" prefWidth="439.0" spacing="20.0" styleClass="Primary" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.Frenadol.View.PrimaryController">
   <children>
      <Label text="TELANOVA">
         <font>
            <Font name="Constantia" size="40.0" />
         </font>
      </Label>
      <Label text="Bienvenido/a a Telanova">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="registerButton" onAction="#switchToRegister" prefHeight="38.0" prefWidth="99.0" text="Crear Cuenta" />
      <Button fx:id="sessionButton" onAction="#switchToStartSession" prefHeight="38.0" prefWidth="99.0" text="Iniciar Sesion" />
   </children>
</VBox>
